<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3_Female_Workout">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#21AAF3"
        android:overScrollMode="never"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <ImageButton
                android:id="@+id/button1"
                android:layout_width="wrap_content"
                android:layout_height="1in"
                android:layout_margin="5dp"
                android:background="#21AAF3"
                android:elevation="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/pushup"
                tools:ignore="SpeakableTextPresentCheck" />




            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button2"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/pull_ups"
                    tools:ignore="SpeakableTextPresentCheck" />



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button3"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/dips"
                    tools:ignore="SpeakableTextPresentCheck" />



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button4"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/bodyweight_squats"
                    tools:ignore="SpeakableTextPresentCheck" />



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button5"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/lunges"
                    tools:ignore="SpeakableTextPresentCheck" />



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button6"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/plank_male" />



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button7"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/crunches" />



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                app:cardBackgroundColor="#21AAF3"
                app:cardCornerRadius="5dp"
                app:cardElevation="5dp">

                <ImageButton
                    android:id="@+id/button8"
                    android:layout_width="wrap_content"
                    android:layout_height="1in"
                    android:layout_margin="5dp"
                    android:background="#21AAF3"
                    android:elevation="3dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/mountian_climb" />



            </androidx.cardview.widget.CardView>



        </LinearLayout>
    </ScrollView>
    <Button
        android:id="@+id/button_backhome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#162FDA"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />
</androidx.constraintlayout.widget.ConstraintLayout>
